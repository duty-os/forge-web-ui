{
  "name": "forge-web-ui",
  "version": "1.0.0",
  "description": "A lightweight Web Components toolbar library for whiteboard applications",
  "type": "module",
  "main": "./dist/forge-web-ui.umd.js",
  "module": "./dist/forge-web-ui.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite --config vite.config.dev.ts",
    "build": "tsc && vite build",
    "build:types": "tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "web-components",
    "toolbar",
    "whiteboard",
    "custom-elements",
    "forge",
    "netless"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^25.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "dependencies": {
    "@netless/forge-room": "^1.1.1",
    "@netless/forge-rtm": "^1.1.1",
    "@netless/forge-whiteboard": "^1.1.1",
    "agora-rtm": "2.2.3-2"
  },
  "peerDependencies": {
    "@netless/forge-whiteboard": "^1.1.1"
  }
}
